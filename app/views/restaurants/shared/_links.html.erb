<%- if controller_name != 'sessions' %>
  <%= link_to "Ya tienes cuenta? Inicia sesion", new_session_path(resource_name) %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Eres nuevo? Crea una cuenta", new_registration_path(resource_name) %>
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Olvidaste tu contrasena?", new_password_path(resource_name) %>
<% end %>
